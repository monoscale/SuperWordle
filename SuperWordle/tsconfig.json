    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "lib": [ "es6", "dom", "es2016" ],
    "target": "es5",
    "outDir": "wwwroot/js",
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "Scripts/**/*"
  ]
}
